{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "app",
            "image": "aurorawr/new-teammaker",
            "hostname": "app",
            "essential": true,
            "memory": 512
        },
        {
            "name": "db",
            "image": "postgres",
            "hostname": "db",
            "essential": true,
            "memory": 512,
            "environment": [
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "tm1234"
                }
            ]
        },
        {
            "name": "web",
            "image": "aurorawr/aurorapps-nginx",
            "hostname": "web",
            "essential": true,
            "memory": 512,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ]
        }
    ]
}